use spring

create table user_account(
user_id varchar(20),
user_name varchar(30),
password varchar(20),
postcode varchar(8),
address varchar(50),
email varchar(50),

job varchar(30),
birthday date,
primary key(user_id)
);

c:\_bb\user.csv

delete from user_account;

load data local infile 'c:\\_bb\\user.csv'
into table user_account
fields terminated by ','
lines terminated by '\r\n';

select * from user_account;